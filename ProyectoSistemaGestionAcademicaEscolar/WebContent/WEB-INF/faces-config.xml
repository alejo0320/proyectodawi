<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">
    
    <validator>
        <validator-id>passwordValidator</validator-id>
        <validator-class>utiles.PasswordValidator</validator-class>
    </validator>
    
    <validator>
        <validator-id>exoneracionValidatorParte1</validator-id>
        <validator-class>utiles.ExoneracionValidatorParte1</validator-class>
    </validator>
    
    <validator>
        <validator-id>exoneracionValidatorParte2</validator-id>
        <validator-class>utiles.ExoneracionValidatorParte2</validator-class>
    </validator>
    
    <validator>
        <validator-id>asignaturaValidator</validator-id>
        <validator-class>utiles.AsignaturaValidator</validator-class>
    </validator>
    
    <managed-bean>
		<description>scheduleController</description>
		<managed-bean-name>scheduleController</managed-bean-name>
		<managed-bean-class>org.primefaces.event.ScheduleController</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
    
  

</faces-config>
